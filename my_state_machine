digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	idle [label="idle\l" color=red fillcolor=darksalmon peripheries=2]
	knife_up_first [label="knife_up_first\l"]
	infra [label="infra\l"]
	feed_forward [label="feed_forward\l"]
	knife_down [label="knife_down\l"]
	knife_up_second [label="knife_up_second\l"]
	paused [label="paused\l"]
	stopped [label="stopped\l"]
	idle -> knife_up_first [label=start]
	idle -> paused [label=pause]
	idle -> stopped [label=stop]
	knife_up_first -> infra [label=infra_start]
	knife_up_first -> paused [label=pause]
	knife_up_first -> stopped [label=stop]
	infra -> feed_forward [label=infra_complete]
	infra -> paused [label=pause]
	infra -> stopped [label=stop]
	feed_forward -> knife_down [label=feed_complete]
	feed_forward -> paused [label=pause]
	feed_forward -> stopped [label=stop]
	knife_down -> knife_up_second [label=knife_down_complete]
	knife_down -> paused [label=pause]
	knife_down -> stopped [label=stop]
	knife_up_second -> paused [label=pause]
	knife_up_second -> stopped [label=stop]
	paused -> paused [label=pause]
	paused -> knife_down [label=resume]
	paused -> stopped [label=stop]
	stopped -> paused [label=pause]
	stopped -> stopped [label=stop]
}
